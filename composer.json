{
    "name": "dewsign/social-links",
    "description": "Allows for the easy addition of social links to a model.",
    "type": "boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Crewdson",
            "email": "danielcrewdson@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "laravel/nova": "^1.1",
        "dewsign/nova-repeater-blocks": "^1.3",
        "dereuromark/media-embed": "^0.4.4",
        "timothyasp/nova-color-field": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Dewsign\\SocialLinks\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dewsign\\SocialLinks\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
